# Matrix A
A <- matrix(c(2,0,1,3), ncol=2)
# Matrix B
B <- matrix(c(5,2,4,-1), ncol=2)
# Addition of A and B
A + B
# Subtraction of A and B
A - B
mat_vals <- c(3, 2, 2, 2, 2, 1, 3, 0, 0, 0, 1, 0, 3, 0, 0, 1, 0, 3, 0, 0, 3)
A <- matrix(mat_vals, nrow = 5)
B <- diag(c(1, 1, 1, 1, 1))
result <- A %*% B
result
